<!-- Page Content -->
<section id="works" class="section-padding wow fadeInUp delay-05s">
<div class="container">

    <div class="row">

        <!-- Blog Entries Column -->
        <div class="row">
            {{#each test}}
            <!-- First Blog Post -->
            <br>
            <h2>
                <a href="#">{{title}}</a>
            </h2>
            <p class="lead">
                by <a href="index.php">{{auther}}</a>
            </p>
            <p><span class="glyphicon glyphicon-time"></span>{{posted}}</p>
            <hr>
            <img class="img-responsive" src="http://placehold.it/900x300" alt="">
            <hr>
            <p style="text-overflow: ellipsis; white-space: nowrap; overflow: hidden;">{{post}}</p>
            <a class="btn btn-primary" href="/blogs/{{_id}}">Read More <span class="glyphicon glyphicon-chevron-right"></span></a>
            <p style="visibility: hidden;">{{_id}}</p>
            <hr>
            {{/each}}
            <!-- Pager -->
            <ul class="pager">
                <li class="previous">
                    <a href="#">&larr; Older</a>
                </li>
                <li class="next">
                    <a href="#">Newer &rarr;</a>
                </li>
            </ul>

        </div>
        {{test.title}}

